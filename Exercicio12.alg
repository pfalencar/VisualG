algoritmo "Exercicio_12"
var
  valorSegundos,valorHoras,valorMinutos:real
inicio
  escreval("Converte segundos em horas e minutos")
  escreval("Digite o valor em segundos:")
  leia(valorSegundos)
  valorHoras<-valorSegundos/3600
  valorMinutos<-valorSegundos/60
  escreval("O valor em horas é: ",valorHoras)
  escreval("O valor em minutos é:",valorMinutos)
fimalgoritmo
