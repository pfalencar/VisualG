algoritmo "Exercicio_7"
var
  ANONASC,ANOATUAL,IDADEATUAL,IDADE2050:inteiro
inicio
  escreval("Digite o ano atual:")
  leia(ANOATUAL)
  escreval("Digite o seu ano de nascimento:")
  leia(ANONASC)
  IDADEATUAL<-ANOATUAL-ANONASC
  escreval("Sua idade atual é: ",IDADEATUAL)
  IDADE2050<-2050-ANONASC
  escreval("Sua idade em 2050 será: ",IDADE2050)
fimalgoritmo
