algoritmo "Exercicio_15"
var
  AREALATA,RAIO,ALTURA:real
inicio
  escreval("Calcula a área da lata")
  escreval("Digite o valor do raio:")
  leia(RAIO)
  escreval("Digite o valor da altura:")
  leia(ALTURA)
  AREALATA<-(Pi*RAIO*RAIO)+(2*RAIO*ALTURA)
  escreval("A área da lata é: ",AREALATA)
fimalgoritmo
